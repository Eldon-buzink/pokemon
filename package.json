{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "seed:celebrations": "ts-node --transpile-only src/scripts/seedCelebrations.ts",
    "seed:history": "ts-node --transpile-only src/scripts/seed-history.ts",
    "create:tables": "ts-node --transpile-only src/scripts/create-history-tables.ts",
    "debug:ppt": "ts-node --transpile-only src/scripts/debugPPT.ts",
    "find:celebrations": "ts-node --transpile-only src/scripts/findCelebrations.ts",
    "migrate:db": "ts-node --transpile-only src/scripts/migrateDatabaseViews.ts",
    "ingest:modern-sets": "ts-node --transpile-only src/scripts/ingest-modern-sets.ts",
    "ingest:all-sets": "ts-node --transpile-only src/scripts/ingest-all-sets.ts",
    "compute:aggregates": "ts-node --transpile-only src/scripts/compute-aggregates.ts",
    "expand:sets": "ts-node --transpile-only src/scripts/gradual-set-expansion.ts",
    "ingest": "ts-node --transpile-only src/scripts/ingest.ts",
    "ingest:modern": "tsx src/scripts/ingest-modern-sets.ts",
    "ingest:all": "tsx src/scripts/ingest-all-sets.ts",
    "ingest:expand": "tsx src/scripts/gradual-set-expansion.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "add:next-set": "node add-remaining-sets.js",
    "refresh:ebay:set": "tsx src/scripts/refresh-ebay-prices.ts --set",
    "refresh:ebay:all": "tsx src/scripts/refresh-ebay-prices.ts --all",
    "ebay:robust": "tsx src/scripts/refresh-ebay-robust.ts",
    "ebay:test": "tsx src/scripts/refresh-ebay-robust.ts --set cel25c --window 7 --budget 50 --resume",
    "build:prod": "NODE_ENV=production npm run build",
    "start:prod": "NODE_ENV=production npm start",
    "health:check": "tsx src/scripts/health-check.ts",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/xml2js": "^0.4.14",
    "apify-client": "^2.17.0",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "lucide-react": "^0.543.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.0",
    "tailwind-merge": "^3.3.1",
    "xml2js": "^0.6.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "vitest": "^1.0.0"
